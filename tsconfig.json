{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "lib": [],
    "jsx": "react",
    "jsxFactory": "h",
    "sourceMap": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts"
  ]
}